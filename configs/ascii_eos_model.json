    "use_z_loss": true,
    "z_loss_coefficient": 1e-4,

    "model_config": {
        "vocab_size": 256,
        "dim": 768,
        "num_layers": 4,
        "headcount": 12,
        "dim_head": 64,
        "ff_mult": 4,
        "lambda": true, 
        "layerwisenorm": "rmsnorm",
        "qknorm": "dynamic_shape_rmsnorm",
        "attention_deux": false,
        "training_seqlen": 512 
    }
