{
    "input_bin": "data/tinystories-ascii-eos/tinystories-ascii-eos_train_*.bin",
    "input_val_bin": "data/tinystories-ascii-eos/tinystories-ascii-eos_val_*.bin",
    "run_name": "ascii-eos-L4-D768-A",

    "batch_size": 512, 
    "device_batch_size": 128, 
    "sequence_length": 512, 
    "num_iterations": 4500, 
    "attack":40,
